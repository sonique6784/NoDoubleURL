{
	"manifest_version": 3,
	"name": "TabRecycle",
	"description": "aim to make sure each URL is open only once",
	"version": "0.1",
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"service_worker": "assets/javascript/TabRecycle.js"
	},
	"action": {
		"default_icon": {
			"19": "assets/images/tab-recycle-disabled-19.png",
			"38": "assets/images/tab-recycle-disabled-38.png"
		},
		"default_title": "TabRecycle"
	},
	"options_ui": {
		"page": "assets/options.html",
		"open_in_tab": false
	},
	"homepage_url": "https://github.com/sonique6784/TabRecycle",
	"icons": {
		"128": "assets/images/tab-recycle-128.png",
		"148": "assets/images/tab-recycle-148.png"
	}
}